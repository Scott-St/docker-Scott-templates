<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
<Category>FTP:</Category>
  <Name>lftp Seedbox Sync</Name>
  <Overview>
   lftp Seedbox Sync Docker
  </Overview>
  <Description>
  </Description>
  <Registry>https://hub.docker.com/r/scottst/lftp/</Registry>
  <GitHub>https://github.com/ZPI-Scott/lftp</GitHub>
  <Support>http://lime-technology.com/forum/</Support>
  <Repository>scottst/lftp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Environment>
    <Variable>
      <Name>FTP_PORT</Name>
      <Value>990</Value>
    </Variable>
    <Variable>
      <Name>FTP_USER</Name>
      <Value>username</Value>
    </Variable>
    <Variable>
      <Name>FTP_PASSWORD</Name>
      <Value>password</Value>
    </Variable>
    <Variable>
      <Name>FTP_HOST</Name>
      <Value>host</Value>
    </Variable>
  </Environment>
  <Icon>http://windows7vswindows8.com/wp-content/uploads/2011/12/ftp_icon.png</Icon>
</Container>
