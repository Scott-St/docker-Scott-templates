<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
<Category>FTP:</Category>
  <Name>lftp Seedbox Sync</Name>
  <Overview>
   lftp Seedbox Sync Docker
  </Overview>
  <Description>
  </Description>
  <Registry>https://hub.docker.com/r/scottst/lftp/</Registry>
  <GitHub>https://github.com/ZPI-Scott/lftp</GitHub>
  <Support>http://lime-technology.com/forum/</Support>
  <Repository>scottst/lftp</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
  </Networking>
  <Environment>
    <Variable>
      <Name>FTP_PORT</Name>
      <Value>990</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/lftp</HostDir>
      <ContainerDir>/mnt/appdata</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/Downloads</HostDir>
      <ContainerDir>/mnt/Downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>http://windows7vswindows8.com/wp-content/uploads/2011/12/ftp_icon.png</Icon>
</Container>
